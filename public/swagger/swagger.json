    "paths": {
        "/opensearch": {
            "get": {
                "description": "Generic Search for Actionable Product",
                "operationId": "opensearch",
                "parameters": [
                    {
                        "default": "precip_3hrs",
                        "description": "Product query",
                        "enum": [
                            "precip_30mn",
                            "precip_3hrs",
                            "precip_1d",
                            "precip_7d"
                        ],
                        "in": "query",
                        "name": "q",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "default": 0,
                        "description": "Latitude",
                        "in": "query",
                        "maximum": 90.0,
                        "minimum": -90.0,
                        "name": "lat",
                        "required": true,
                        "type": "number"
                    },
                    {
                        "default": 0,
                        "description": "Longitude",
                        "in": "query",
                        "maximum": 180.0,
                        "minimum": -180.0,
                        "name": "lon",
                        "required": true,
                        "type": "number"
                    },
                    {
                        "default": 2,
                        "description": "Limit number of responses",
                        "in": "query",
                        "maximum": 1000,
                        "minimum": 1,
                        "name": "limit",
                        "required": true,
                        "type": "integer"
                    },
                    {
                        "default": "gpm",
                        "description": "Product sources",
                        "enum": [
                            "gpm",
                            "trmm"
                        ],
                        "in": "query",
                        "name": "sources",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "description": "Start Time",
                        "format": "date",
                        "in": "query",
                        "name": "startTime",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "description": "End Time",
                        "format": "date",
                        "in": "query",
                        "name": "endTime",
                        "required": false,
                        "type": "string"
                    }
                ],
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                },
                "summary": "Generic OpenSearch",
                "tags": [
                    "opensearch"
                ]
            }
        },
